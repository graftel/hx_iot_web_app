<html>
<%- include ../partials/header.ejs %> <%- include
../partials/scripts.ejs %>
<body>
	<div id="wrapper">
		<!-- Navigation -->
		<nav class="navbar navbar-default navbar-static-top" role="navigation"
			style="margin-bottom: 0"><%- include ../partials/header-nav.ejs
			%> <%- include ../partials/side-nav.ejs %></nav>
		<div id="page-wrapper">
			<h3 style="margin: 0; padding-top: 2%;">Settings</h3>
			<form id="settings-form" action="/settings" method="post" role="form"
				style="display: block;">
				<div class="panel panel-info col-md-4" style="margin-top: 2%;">
					<div class="panel-heading">Dashboard Settings</div>
					<div class="panel-body">
						<div class="form-group row">
							<div class="col-md-4">
								<label>Main Parameter</label>
							</div>
							<div class="col-md-8">
								<select id="main-param" name="main-param">
								<% settingOptions.forEach(function(o){ %>
								<% if(o == mainParam){ %>
									<option selected="selected"> <%= o %> </option>
								<% } else { %>
								<option> <%= o %> </option>
								<%} }); %>
								</select>
							</div>
						</div>
						<div class="form-group">
							<div class="col-sm-6 col-sm-offset-3">
								<input type="submit" tabindex="4"
									class="form-control btn btn-primary" value="Update">
							</div>
						</div>
					</div>
			</form>
		</div>
	</div>
</body>
</html>
